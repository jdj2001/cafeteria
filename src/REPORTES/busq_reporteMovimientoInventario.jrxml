	<queryString>
		<![CDATA[SELECT
     producto.`costo` AS producto_costo,
     factura.`cantidad` AS factura_cantidad,
     inventario.`idProducto` AS inventario_idProducto,
     inventario.`descripcion` AS inventario_descripcion,
     producto.`estado` AS producto_estado,
     producto.`utilidad` AS producto_utilidad,
     producto.`precioVenta` AS producto_precioVenta,
     categoria.`descripcion` AS categoria_descripcion,
     factura.`fecha` AS factura_fecha
FROM
     `producto` producto INNER JOIN `inventario` inventario ON producto.`id` = inventario.`idProducto`
     INNER JOIN `categoria` categoria ON producto.`idCategoria` = categoria.`id`
     INNER JOIN `factura` factura ON producto.`id` = factura.`codigoProducto`
WHERE factura.`codigoProducto` LIKE $P{PARAMETRO}
OR factura.`producto` LIKE $P{PARAMETRO}
OR factura.`cantidad` LIKE $P{PARAMETRO}]]>
	</queryString>
