	<subDataset name="PROVEEDOR" uuid="34ef98dc-fa38-47dc-83ab-b47795a82ca1">
		<queryString>
			<![CDATA[SELECT
     proveedor.`id` AS proveedor_id,
     proveedor.`descripcion` AS proveedor_descripcion,
     proveedor.`ciudad` AS proveedor_ciudad,
     proveedor.`telefono` AS proveedor_telefono,
     proveedor.`estado` AS proveedor_estado,
     producto.`descripcion` AS producto_descripcion,
     categoria.`descripcion` AS categoria_descripcion
FROM
     `proveedor` proveedor INNER JOIN `producto` producto ON proveedor.`id` = producto.`idProveedor`
     INNER JOIN `categoria` categoria ON producto.`idCategoria` = categoria.`id`]]>
		</queryString>
