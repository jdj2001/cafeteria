	<queryString>
		<![CDATA[SELECT
     SUM(factura.`cantidad`) AS cantidad_total_vendida,
     factura.`producto` AS factura_producto,
     factura.`cantidad` AS factura_cantidad,
     factura.`fecha` AS factura_fecha
FROM
     `factura` factura
WHERE
     factura.`fecha` BETWEEN $P{fechaIni} AND $P{fechaFin}
GROUP BY
     factura.`producto`]]>
