	<subDataset name="PRODUCTO" uuid="b9458302-8907-4cf7-b503-1f17e0f2406a">
		<queryString language="SQL">
			<![CDATA[SELECT
     producto.`id` AS producto_id,
     producto.`descripcion` AS producto_descripcion,
     producto.`costo` AS producto_costo,
     producto.`utilidad` AS producto_utilidad,
     producto.`precioVenta` AS producto_precioVenta,
     producto.`CantidadMaxima` AS producto_CantidadMaxima,
     producto.`CantidadMinima` AS producto_CantidadMinima,
     producto.`estado` AS producto_estado,
     categoria.`descripcion` AS categoria_descripcion,
     proveedor.`descripcion` AS proveedor_descripcion
FROM
     `categoria` categoria INNER JOIN `producto` producto ON categoria.`id` = producto.`idCategoria`
     INNER JOIN `proveedor` proveedor ON producto.`idProveedor` = proveedor.`id`]]>
		</queryString>
